<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <welcome-file-list>
        <welcome-file>Welcome.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>wxcachecountsend</servlet-name>
        <servlet-class>com.tsinghua.WxCacheCountSend</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>wxcachecountsend</servlet-name>
        <url-pattern>/wxcachecountsend</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>wxgettokenandticket</servlet-name>
        <servlet-class>com.tsinghua.WxGetTokenAndTicket</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>wxgettokenandticket</servlet-name>
        <url-pattern>/wxgettokenandticket</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>wxconnect</servlet-name>
        <servlet-class>com.tsinghua.wxConnect</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>wxconnect</servlet-name>
        <url-pattern>/wxconnect</url-pattern>
    </servlet-mapping>
</web-app>