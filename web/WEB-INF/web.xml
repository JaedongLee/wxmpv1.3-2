<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <welcome-file-list>
        <welcome-file>Welcome.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>wxsenddb</servlet-name>
        <servlet-class>com.tsinghua.wxsendDB</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>wxsenddb</servlet-name>
        <url-pattern>/wxsenddb</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>wxgettokenandticket</servlet-name>
        <servlet-class>com.tsinghua.WxGetTokenAndTicket</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>wxgettokenandticket</servlet-name>
        <url-pattern>/wxgettokenandticket</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>wxconnect</servlet-name>
        <servlet-class>com.tsinghua.wxConnect</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>wxconnect</servlet-name>
        <url-pattern>/wxconnect</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>wxpay</servlet-name>
        <servlet-class>com.tsinghua.wxpay</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>wxpay</servlet-name>
        <url-pattern>/wxpay</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>wxlayindb</servlet-name>
        <servlet-class>com.tsinghua.wxlayinDB</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>wxlayindb</servlet-name>
        <url-pattern>/wxlayindb</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>wxdbproxy</servlet-name>
        <servlet-class>com.tsinghua.wxDBProxy</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>wxdbproxy</servlet-name>
        <url-pattern>/wxdbproxy</url-pattern>
    </servlet-mapping>

</web-app>